<?xml version="1.0" encoding="us-ascii"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="sqaleManager">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="rules">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="rule">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="name" type="xs:string" />
                                        <xs:element name="requirement">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <!--PORTABILITY-->
                                                    <xs:enumeration value="COMPILER_RELATED_PORTABILITY"/>
                                                    <xs:enumeration value="HARDWARE_RELATED_PORTABILITY"/>
                                                    <xs:enumeration value="LANGUAGE_RELATED_PORTABILITY"/>
                                                    <xs:enumeration value="OS_RELATED_PORTABILITY"/>
                                                    <xs:enumeration value="SOFTWARE_RELATED_PORTABILITY"/>
                                                    <xs:enumeration value="TIME_ZONE_RELATED_PORTABILITY"/>
                                                    <!--MAINTAINABILITY-->
                                                    <xs:enumeration value="READABILITY"/>
                                                    <xs:enumeration value="UNDERSTANDABILITY"/>
                                                    <!--SECURITY-->
                                                    <xs:enumeration value="API_ABUSE"/>
                                                    <xs:enumeration value="ERRORS"/>
                                                    <xs:enumeration value="INPUT_VALIDATION_AND_REPRESENTATION"/>
                                                    <xs:enumeration value="SECURITY_FEATURES"/>
                                                    <!--EFFICIENCY-->
                                                    <xs:enumeration value="MEMORY_EFFICIENCY"/>
                                                    <xs:enumeration value="CPU_EFFICIENCY"/>
                                                    <!--CHANGEABILITY-->
                                                    <xs:enumeration value="ARCHITECTURE_CHANGEABILITY"/>
                                                    <xs:enumeration value="LOGIC_CHANGEABILITY"/>
                                                    <!--RELIABILITY-->
                                                    <xs:enumeration value="ARCHITECTURE_RELIABILITY"/>
                                                    <xs:enumeration value="DATA_RELIABILITY"/>
                                                    <xs:enumeration value="EXCEPTION_HANDLING"/>
                                                    <xs:enumeration value="INSTRUCTION_RELIABILITY"/>
                                                    <xs:enumeration value="LOGIC_RELIABILITY"/>
                                                    <xs:enumeration value="SYNCHRONIZATION_RELIABILITY"/>
                                                    <xs:enumeration value="UNIT_TESTS"/>
                                                    <!--TESTABILITY-->
                                                    <xs:enumeration value="INTEGRATION_TESTABILITY"/>
                                                    <xs:enumeration value="UNIT_TESTABILITY"/>
                                                    <!--REUSABILITY-->
                                                    <xs:enumeration value="MODULARITY"/>
                                                    <xs:enumeration value="TRANSPORTABILITY"/>
                                                    <!--		define place holder which will not be considered for transcription/>-->
                                                    <xs:enumeration value="undefined"/>
<!-- name attributes for characteristics -->
